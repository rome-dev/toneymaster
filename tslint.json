{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "no-var-keyword": true,
    "no-unused-variable": true,
    "no-shadowed-variable": true,
    "no-console": true
  },
  "linterOptions": {
    "exclude": ["src/serviceWorker.ts"]
  }
}
